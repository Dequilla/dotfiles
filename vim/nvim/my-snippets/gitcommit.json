{
    "Git Development": {
        "prefix": "FD-",
        "body": [
            "FD-$1 - $0"
        ],
        "description": "Development prefix"
    },
    "Git Internal": {
        "prefix": "FI-",
        "body": [
            "FI-$1 - $0"
        ],
        "description": "Internal prefix"
    },
    "co-authored by": {
        "prefix": "co",
        "body": [
            "Co-authored-by: ${1:name} <${2:email}>",
            "$0"
        ]
    },
    "signed off by": {
        "prefix": "si",
        "body": [
            "Signed-off-by: ${1:name} <${2:email}>", 
            "$0"
        ]
    },
    "on behalf of": {
        "prefix": "on",
        "body": [
            "On-behalf-of: ${1:org} <${2:email}>",
            "$0"
        ]
    }
}
